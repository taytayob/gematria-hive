modules = ["python-3.12"]

[env]
PYTHON_VERSION = "3.12"
SUPABASE_URL = "your_supabase_url_here"
SUPABASE_KEY = "your_anon_key_here"

[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["glibcLocales"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Streamlit App"

[[workflows.workflow]]
name = "Streamlit App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 44603
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = "streamlit run app.py --server.port 5000"
